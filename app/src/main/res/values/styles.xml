<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="tv_iv_main">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="tv_standard">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">10sp</item>
    </style>
    <style name="tv_standard_match">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">10sp</item>
    </style>
    <style name="tv_harvestaddress">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="custom_dialog_style" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item><!--除去title-->
        <item name="android:windowBackground">@color/transparent</item><!--除去背景色-->
        <item name="android:radius">10dp</item>
    </style>

    <style name="tv_grey">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#656565</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="view_standard">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.6dp</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_marginTop">7dp</item>
        <item name="android:layout_marginBottom">7dp</item>
    </style>

    <style name="text_qsmalllist">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">10dp</item>
        <item name="android:paddingLeft">8dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:background">@drawable/stroke_gray_nonesolid</item>
        <item name="android:layout_marginRight">8dp</item>
    </style>

    <style name="edit_standred">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">@null</item>
    </style>

    <style name="tv_iv_setting">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/tv_ranking</item>
        <item name="android:button">@null</item>
        <item name="android:textSize">11sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="checkboxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_style</item>
    </style>

    <style name="edt_addproduct">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">10sp</item>
        <item name="android:background">@null</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="view_address">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.3dp</item>
        <item name="android:background">@color/view</item>
        <item name="android:layout_margin">8dp</item>
    </style>

    <style name="TabLayoutTextStyle">
        <item name="android:textSize">10sp</item>
    </style>

    <style name="view_padding8dp">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.2dp</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:background">@color/view</item>
    </style>

    <style name="editText_adaress">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="ll_consumptionrecord">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginRight">30dp</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:gravity">center_vertical</item>
    </style>

    <style name="tv_entrep">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/red</item>
    </style>

    <style name="edt_register">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:background">@null</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="ll_register">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingTop">15dp</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingBottom">15dp</item>
    </style>


    <declare-styleable name="GifView">
        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomTheme">
        <attr name="gifViewStyle" format="reference" />
    </declare-styleable>

    <style name="Widget_GifView"></style>
</resources>
